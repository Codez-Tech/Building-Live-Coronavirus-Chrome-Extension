{
    "manifest_version": 2,
    "name": "Live Coronavirus status",
    "description": "Real time live Coronavirus updates",
    "version": "1.0.0",
    "icons": { "128": "./assets/images/coronavirus-logo.png" },
    "icons1": {
      "16": "icon/background/icon_16.png",
      "48": "icon/background/icon_48.png",
      "64": "icon/background/icon_64.png",
      "128": "icon/background/icon_128.png"
  },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "css": ["./assets/css/jquery.dataTables.css", "./assets/css/shCore.css", "./assets/css/demo.css"],
        "scripts": [
            "./assets/js/jquery.js",
            "./assets/js/shCore.js",  
            "./assets/js/demo.js", 
            "./assets/js/jquery.dataTables.js"
        ],
        "run_at": "document_start"
    }],
    "browser_action": {
      "default_icon": "./assets/images/coronavirus-logo.png",
      "default_popup": "index.html"
    },
    "permissions": ["activeTab"],
    "content_security_policy": "default-src 'self'; style-src 'unsafe-inline'; script-src 'self';",
    "content_security_policy2": "script-src 'self'; object-src 'self';style-src 'unsafe-inline'; font-src 'self' https://fonts.googleapis.com;",
    "content_security_policy3": "script-src 'self'; object-src 'self' 'sha256-xgSe9e4bUQlmSCi8bnI5e18dJWMjs4ywwBD+uK3ci/o=';"
    
  }